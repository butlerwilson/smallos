# This is a configure file for bochs.

# filename of ROM image
romimage: file=$BXSHARE/BIOS-bochs-latest 
vgaromimage: file=$BXSHARE/VGABIOS-lgpl-latest

# what disk image will be used
floppya: 1_44=a.img, status=inserted

# chode the boot disk
boot: floppy
#boot: a
#boot: disk

# ths other information
log: bochsout.txt
mouse: enabled=0
megs: 32
